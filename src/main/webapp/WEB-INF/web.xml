<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
    <display-name>weblab2</display-name>
    <servlet>
        <servlet-name>ControllerServlet</servlet-name>
        <servlet-class>com.example.weblab2.servlets.ControllerServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ControllerServlet</servlet-name>
        <url-pattern>/controllerServlet</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>AreaCheckServlet</servlet-name>
        <servlet-class>com.example.weblab2.servlets.AreaCheckServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ClearTableServlet</servlet-name>
        <servlet-class>com.example.weblab2.servlets.ClearTableServlet</servlet-class>
    </servlet>
    <welcome-file-list>
        <welcome-file>index.jsp</welcome-file>
    </welcome-file-list>
</web-app>
